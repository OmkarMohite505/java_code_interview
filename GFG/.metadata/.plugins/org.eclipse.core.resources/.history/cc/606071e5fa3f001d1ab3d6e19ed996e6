package Array;

public class LeftShiftBy1 {

	public static void leftShiftByOne(int arr[]) {
		int first = arr[0];
		int len = arr.length;
		for (int i = 0; i < arr.length - 1; i++) {
			arr[i] = arr[i + 1];
			System.out.print(arr[i] + " ");
		}
		arr[len - 1] = first;
	}

	public static void main(String[] args) {

		int arr[] = { 1, 2, 5, 7, 9, 8 };
		leftShiftByOne(arr);
	}

}
